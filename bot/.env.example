# Environment variables for Narnia Bot
# Copy this to .env and modify as needed

# Bot Configuration
BOT_ID=auto_generated
BOT_NAME=NarniaBot
CHECKIN_INTERVAL=30

# C2 Server Configuration
C2_URL=http://localhost:8000
BACKUP_C2_URLS=http://backup1.example.com:8000,http://backup2.example.com:8000

# Communication Settings
TOR_ENABLED=false
TOR_PROXY=socks5://127.0.0.1:9050
DNS_ENABLED=false
DNS_DOMAIN=botnet.example.com

# Security Settings
ENCRYPTION_ENABLED=false
ENCRYPTION_KEY=change_this_encryption_key_32_chars_minimum

# Module Enablement
DDOS_ENABLED=true
MINING_ENABLED=true
WORM_ENABLED=true
PROXY_ENABLED=true
EXFIL_ENABLED=true
RANSOMWARE_ENABLED=false
P2P_ENABLED=true

# Security Features
ANTI_VM_ENABLED=true
ETW_BYPASS_ENABLED=false
AMSI_BYPASS_ENABLED=false
FILELESS_EXECUTION=false
POLYMORPHIC_ENABLED=false
SELF_DELETE_ON_DETECTION=false

# Network Settings
USER_AGENT=NarniaBot/0.1.0
TIMEOUT_SECONDS=30
MAX_CONNECTIONS=10
PROXY_SOCKS5_ADDR=
P2P_LISTEN_PORT=8081
P2P_BOOTSTRAP_PEERS=192.168.1.100:8081,192.168.1.101:8081

# Retry Settings
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY_SECONDS=10
